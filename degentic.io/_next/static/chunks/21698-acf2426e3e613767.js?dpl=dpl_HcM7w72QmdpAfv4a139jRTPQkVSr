                }
                update(e) {
                    ir(this);
                    let {
                        buffer: t,
                        blockLen: i
                    } = this, n = (e = il(e)).length;
                    for (let r = 0; r < n;) {
                        let o = Math.min(i - this.pos, n - r);
                        if (o === i) {
                            for (; i <= n - r; r += i) this.process(e, r);
                            continue
