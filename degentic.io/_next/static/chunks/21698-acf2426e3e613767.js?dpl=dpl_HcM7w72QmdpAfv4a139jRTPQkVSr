            let eh = function(e) {
                if (e.length >= 255) throw TypeError("Alphabet too long");
                let t = new Uint8Array(256);
                for (let e = 0; e < t.length; e++) t[e] = 255;
                for (let i = 0; i < e.length; i++) {
                    let n = e.charAt(i),
                        r = n.charCodeAt(0);
                    if (255 !== t[r]) throw TypeError(n + " is ambiguous");
                    t[r] = i
                }
                let i = e.length,
                    n = e.charAt(0),
                    r = Math.log(i) / Math.log(256),
                    o = Math.log(256) / Math.log(i);

                function s(e) {
                    if ("string" != typeof e) throw TypeError("Expected String");
                    if (0 === e.length) return new Uint8Array;
                    let o = 0,
                        s = 0,
                        a = 0;
                    for (; e[o] === n;) s++, o++;
                    let l = (e.length - o) * r + 1 >>> 0,
                        c = new Uint8Array(l);
                    for (; o < e.length;) {
                        let n = e.charCodeAt(o);
                        if (n > 255) return;
                        let r = t[n];
                        if (255 === r) return;
                        let s = 0;
                        for (let e = l - 1;
                            (0 !== r || s < a) && -1 !== e; e--, s++) r += i * c[e] >>> 0, c[e] = r % 256 >>> 0, r = r / 256 >>> 0;
                        if (0 !== r) throw Error("Non-zero carry");
                        a = s, o++
                    }
                    let d = l - a;
                    for (; d !== l && 0 === c[d];) d++;
                    let u = new Uint8Array(s + (l - d)),
                        h = s;
                    for (; d !== l;) u[h++] = c[d++];
                    return u
                }
                return {
                    encode: function(t) {
                        if (t instanceof Uint8Array || (ArrayBuffer.isView(t) ? t = new Uint8Array(t.buffer, t.byteOffset, t.byteLength) : Array.isArray(t) && (t = Uint8Array.from(t))), !(t instanceof Uint8Array)) throw TypeError("Expected Uint8Array");
                        if (0 === t.length) return "";
                        let r = 0,
                            s = 0,
                            a = 0,
                            l = t.length;
                        for (; a !== l && 0 === t[a];) a++, r++;
                        let c = (l - a) * o + 1 >>> 0,
                            d = new Uint8Array(c);
                        for (; a !== l;) {
                            let e = t[a],
                                n = 0;
                            for (let t = c - 1;
                                (0 !== e || n < s) && -1 !== t; t--, n++) e += 256 * d[t] >>> 0, d[t] = e % i >>> 0, e = e / i >>> 0;
                            if (0 !== e) throw Error("Non-zero carry");
                            s = n, a++
                        }
                        let u = c - s;
                        for (; u !== c && 0 === d[u];) u++;
                        let h = n.repeat(r);
                        for (; u < c; ++u) h += e.charAt(d[u]);
                        return h
                    },
                    decodeUnsafe: s,
                    decode: function(e) {
                        let t = s(e);
                        if (t) return t;
                        throw Error("Non-base" + i + " character")
                    }
                }
            }("123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz");
