                                rpcOpts: nv.wc_sessionPropose.autoReject
                            }), this.client.logger.error(e)
                        }
                    }), nR(this, "onSessionProposeResponse", async (e, t, i) => {
                        let {
                            id: n
                        } = t;
                        if ((0, ef.isJsonRpcResult)(t)) {
                            let {
                                result: r
                            } = t;
