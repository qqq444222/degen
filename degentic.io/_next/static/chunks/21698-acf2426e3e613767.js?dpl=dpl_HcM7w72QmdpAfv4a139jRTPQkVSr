                            id: o,
                            topic: s,
                            result: a,
                            throwOnFailedPublish: l,
                            encodeOpts: c,
                            appLink: d
                        } = e, u = (0, ef.formatJsonRpcResult)(o, a), h = d && "u" > typeof(null == i.g ? void 0 : i.g.Linking);
                        try {
                            let e = h ? eu.Pa8 : eu.EWt;
                            t = await this.client.core.crypto.encode(s, u, nP(nO({}, c || {}), {
                                encoding: e
                            }))
                        } catch (e) {
                            throw await this.cleanup(), this.client.logger.error(`sendResult() -> core.crypto.encode() for topic ${s} failed`), e
                        }
                        try {
                            let e = (n = await this.client.core.history.get(s, o)).request;
                            try {
                                this.shouldSetTVF(e.method, e.params) && (r = this.getTVFParams(o, e.params, a))
                            } catch (e) {
                                this.client.logger.warn("sendResult() -> getTVFParams() failed", e)
                            }
                        } catch (e) {
                            throw this.client.logger.error(`sendResult() -> history.get(${s}, ${o}) failed`), e
                        }
                        if (h) {
                            let e = (0, eu.$Bq)(d, s, t);
                            await i.g.Linking.openURL(e, this.client.name)
                        } else {
                            let e = nv[n.request.method].res;
                            e.tvf = nP(nO({}, r), {
                                correlationId: o
                            }), l ? (e.internal = nP(nO({}, e.internal), {
                                throwOnFailedPublish: !0
                            }), await this.client.core.relayer.publish(s, t, e)) : this.client.core.relayer.publish(s, t, e).catch(e => this.client.logger.error(e))
                        }
                        await this.client.core.history.resolve(u)
                    }), nR(this, "sendError", async e => {
                        let t, n;
                        let {
                            id: r,
                            topic: o,
                            error: s,
                            encodeOpts: a,
                            rpcOpts: l,
                            appLink: c
                        } = e, d = (0, ef.formatJsonRpcError)(r, s), u = c && "u" > typeof(null == i.g ? void 0 : i.g.Linking);
                        try {
                            let e = u ? eu.Pa8 : eu.EWt;
                            t = await this.client.core.crypto.encode(o, d, nP(nO({}, a || {}), {
                                encoding: e
                            }))
                        } catch (e) {
                            throw await this.cleanup(), this.client.logger.error(`sendError() -> core.crypto.encode() for topic ${o} failed`), e
                        }
                        try {
                            n = await this.client.core.history.get(o, r)
                        } catch (e) {
                            throw this.client.logger.error(`sendError() -> history.get(${o}, ${r}) failed`), e
                        }
                        if (u) {
                            let e = (0, eu.$Bq)(c, o, t);
                            await i.g.Linking.openURL(e, this.client.name)
                        } else {
                            let e = n.request.method,
                                i = l || nv[e].res;
                            this.client.core.relayer.publish(o, t, i)
                        }
                        await this.client.core.history.resolve(d)
                    }), nR(this, "cleanup", async () => {
                        let e = [],
                            t = [];
                        this.client.session.getAll().forEach(t => {
                            let i = !1;
                            (0, eu._dF)(t.expiry) && (i = !0), this.client.core.crypto.keychain.has(t.topic) || (i = !0), i && e.push(t.topic)
                        }), this.client.proposal.getAll().forEach(e => {
                            (0, eu._dF)(e.expiryTimestamp) && t.push(e.id)
                        }), await Promise.all([...e.map(e => this.deleteSession({
                            topic: e
                        })), ...t.map(e => this.deleteProposal(e))])
                    }), nR(this, "onProviderMessageEvent", async e => {
                        !this.initialized || this.relayMessageCache.length > 0 ? this.relayMessageCache.push(e) : await this.onRelayMessage(e)
                    }), nR(this, "onRelayEventRequest", async e => {
                        this.requestQueue.queue.push(e), await this.processRequestsQueue()
                    }), nR(this, "processRequestsQueue", async () => {
                        if (this.requestQueue.state === nj.active) {
                            this.client.logger.info("Request queue already active, skipping...");
                            return
                        }
                        for (this.client.logger.info(`Request queue starting with ${this.requestQueue.queue.length} requests`); this.requestQueue.queue.length > 0;) {
                            this.requestQueue.state = nj.active;
                            let e = this.requestQueue.queue.shift();
                            if (e) try {
                                await this.processRequest(e)
                            } catch (e) {
                                this.client.logger.warn(e)
                            }
                        }
                        this.requestQueue.state = nj.idle
                    }), nR(this, "processRequest", async e => {
                        let {
                            topic: t,
                            payload: i,
                            attestation: n,
                            transportType: r,
                            encryptedId: o
                        } = e, s = i.method;
                        if (!this.shouldIgnorePairingRequest({
                                topic: t,
                                requestMethod: s
                            })) switch (s) {
                            case "wc_sessionPropose":
                                return await this.onSessionProposeRequest({
                                    topic: t,
                                    payload: i,
                                    attestation: n,
                                    encryptedId: o
                                });
                            case "wc_sessionSettle":
                                return await this.onSessionSettleRequest(t, i);
                            case "wc_sessionUpdate":
                                return await this.onSessionUpdateRequest(t, i);
                            case "wc_sessionExtend":
                                return await this.onSessionExtendRequest(t, i);
                            case "wc_sessionPing":
                                return await this.onSessionPingRequest(t, i);
                            case "wc_sessionDelete":
                                return await this.onSessionDeleteRequest(t, i);
                            case "wc_sessionRequest":
                                return await this.onSessionRequest({
                                    topic: t,
                                    payload: i,
                                    attestation: n,
                                    encryptedId: o,
                                    transportType: r
                                });
                            case "wc_sessionEvent":
                                return await this.onSessionEventRequest(t, i);
                            case "wc_sessionAuthenticate":
                                return await this.onSessionAuthenticateRequest({
                                    topic: t,
                                    payload: i,
                                    attestation: n,
                                    encryptedId: o,
                                    transportType: r
                                });
                            default:
                                return this.client.logger.info(`Unsupported request method ${s}`)
                        }
                    }), nR(this, "onRelayEventResponse", async e => {
                        let {
                            topic: t,
                            payload: i,
                            transportType: n
                        } = e, r = (await this.client.core.history.get(t, i.id)).request.method;
                        switch (r) {
                            case "wc_sessionPropose":
                                return this.onSessionProposeResponse(t, i, n);
                            case "wc_sessionSettle":
                                return this.onSessionSettleResponse(t, i);
                            case "wc_sessionUpdate":
                                return this.onSessionUpdateResponse(t, i);
                            case "wc_sessionExtend":
                                return this.onSessionExtendResponse(t, i);
                            case "wc_sessionPing":
                                return this.onSessionPingResponse(t, i);
                            case "wc_sessionRequest":
                                return this.onSessionRequestResponse(t, i);
                            case "wc_sessionAuthenticate":
                                return this.onSessionAuthenticateResponse(t, i);
                            default:
                                return this.client.logger.info(`Unsupported response method ${r}`)
                        }
                    }), nR(this, "onRelayEventUnknownPayload", e => {
