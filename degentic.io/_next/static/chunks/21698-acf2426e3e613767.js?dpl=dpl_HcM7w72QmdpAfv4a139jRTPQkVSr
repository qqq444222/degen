                    l = t.powPminus2 || (e => nb(e, i - BigInt(2), i));

                function c(e, t, i) {
                    let r = n(e * (t - i));
                    return [t = n(t - r), i = n(i + r)]
                }
                let d = (t.a - BigInt(2)) / BigInt(4);

                function u(e, t) {
                    let u = function(e, t) {
                        ne("u", e, nO, i), ne("scalar", t, nO, i);
                        let o = nP,
                            s = nO,
                            a = e,
