                    for (;
                        "=" === e[o - 1];) --o;
                    let s = new Uint8Array(o * i / 8 | 0),
                        a = 0,
                        l = 0,
                        c = 0;
                    for (let t = 0; t < o; ++t) {
                        let o = r[e[t]];
                        if (void 0 === o) throw SyntaxError(`Non-${n} character`);
                        l = l << i | o, (a += i) >= 8 && (a -= 8, s[c++] = 255 & l >> a)
                    }
                    if (a >= i || 255 & l << 8 - a) throw SyntaxError("Unexpected end of data");
                    return s
                },
                k = (e, t, i) => {
                    let n = "=" === t[t.length - 1],
                        r = (1 << i) - 1,
                        o = "",
                        s = 0,
                        a = 0;
                    for (let n = 0; n < e.length; ++n)
                        for (a = a << 8 | e[n], s += 8; s > i;) s -= i, o += t[r & a >> s];
                    if (s && (o += t[r & a << i - s]), n)
                        for (; o.length * i & 7;) o += "=";
                    return o
                },
