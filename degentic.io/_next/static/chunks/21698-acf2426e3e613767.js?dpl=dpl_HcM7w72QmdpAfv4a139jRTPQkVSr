                        }
                        isSmallOrder() {
                            return this.multiplyUnsafe(c).is0()
                        }
                        isTorsionFree() {
                            return I.unsafeLadder(this, r).is0()
                        }
                        toAffine(e) {
                            return b(this, e)
                        }
                        clearCofactor() {
                            let {
                                h: e
                            } = i;
