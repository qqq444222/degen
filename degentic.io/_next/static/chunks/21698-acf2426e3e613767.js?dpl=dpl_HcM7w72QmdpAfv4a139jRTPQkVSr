                    w.BASE = new w(i.Gx, i.Gy, ef, u(i.Gx * i.Gy)), w.ZERO = new w(ep, ef, ef, ep);
                    let {
                        BASE: v,
                        ZERO: M
                    } = w, I = (t = 8 * l, {
                        constTimeNegate: ea,
                        hasPrecomputes: e => 1 !== eh(e),
                        unsafeLadder(e, t, i = w.ZERO) {
                            let n = e;
                            for (; t > eo;) t & es && (i = i.add(n)), n = n.double(), t >>= es;
                            return i
                        },
                        precomputeWindow(e, i) {
                            let {
                                windows: n,
                                windowSize: r
                            } = ec(i, t), o = [], s = e, a = s;
                            for (let e = 0; e < n; e++) {
                                a = s, o.push(a);
                                for (let e = 1; e < r; e++) a = a.add(s), o.push(a);
                                s = a.double()
                            }
                            return o
                        },
                        wNAF(e, i, n) {
                            let {
                                windows: r,
                                windowSize: o
                            } = ec(e, t), s = w.ZERO, a = w.BASE, l = BigInt(2 ** e - 1), c = 2 ** e, d = BigInt(e);
                            for (let e = 0; e < r; e++) {
