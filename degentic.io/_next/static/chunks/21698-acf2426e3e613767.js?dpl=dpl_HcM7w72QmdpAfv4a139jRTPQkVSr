            }, t.toByteArray = function(e) {
                var t, i, o = l(e),
                    s = o[0],
                    a = o[1],
                    c = new r((s + a) * 3 / 4 - a),
                    d = 0,
                    u = a > 0 ? s - 4 : s;
                for (i = 0; i < u; i += 4) t = n[e.charCodeAt(i)] << 18 | n[e.charCodeAt(i + 1)] << 12 | n[e.charCodeAt(i + 2)] << 6 | n[e.charCodeAt(i + 3)], c[d++] = t >> 16 & 255, c[d++] = t >> 8 & 255, c[d++] = 255 & t;
