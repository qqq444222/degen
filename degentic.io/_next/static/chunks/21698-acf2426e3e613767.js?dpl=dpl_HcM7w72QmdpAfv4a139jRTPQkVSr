            function nY(e) {
                void 0 !== e.lowS && iq("lowS", e.lowS), void 0 !== e.prehash && iq("prehash", e.prehash)
            }
            let {
                bytesToNumberBE: nK,
                hexToBytes: nH
            } = nc;
            class nQ extends Error {
                constructor(e = "") {
                    super(e)
                }
            }
            let n$ = {
                    Err: nQ,
                    _tlv: {
                        encode: (e, t) => {
                            let {
                                Err: i
                            } = n$;
                            if (e < 0 || e > 256) throw new i("tlv.encode: wrong tag");
                            if (1 & t.length) throw new i("tlv.encode: unpadded data");
                            let n = t.length / 2,
                                r = iZ(n);
                            if (r.length / 2 & 128) throw new i("tlv.encode: long form length too big");
                            let o = n > 127 ? iZ(r.length / 2 | 128) : "";
                            return iZ(e) + o + r + t
                        },
                        decode(e, t) {
                            let {
                                Err: i
                            } = n$, n = 0;
                            if (e < 0 || e > 256) throw new i("tlv.encode: wrong tag");
                            if (t.length < 2 || t[n++] !== e) throw new i("tlv.decode: wrong tlv");
                            let r = t[n++],
                                o = 0;
                            if (128 & r) {
                                let e = 127 & r;
                                if (!e) throw new i("tlv.decode(long): indefinite length not supported");
                                if (e > 4) throw new i("tlv.decode(long): byte length is too big");
                                let s = t.subarray(n, n + e);
                                if (s.length !== e) throw new i("tlv.decode: length bytes not complete");
                                if (0 === s[0]) throw new i("tlv.decode(long): zero leftmost byte");
                                for (let e of s) o = o << 8 | e;
                                if (n += e, o < 128) throw new i("tlv.decode(long): not minimal encoding")
                            } else o = r;
                            let s = t.subarray(n, n + o);
                            if (s.length !== o) throw new i("tlv.decode: wrong value length");
                            return {
                                v: s,
                                l: t.subarray(n + o)
                            }
                        }
                    },
                    _int: {
                        encode(e) {
                            let {
                                Err: t
                            } = n$;
                            if (e < nq) throw new t("integer: negative integers are not allowed");
                            let i = iZ(e);
                            if (8 & Number.parseInt(i[0], 16) && (i = "00" + i), 1 & i.length) throw new t("unexpected DER parsing assertion: unpadded hex");
                            return i
                        },
                        decode(e) {
                            let {
                                Err: t
                            } = n$;
                            if (128 & e[0]) throw new t("invalid signature integer: negative");
                            if (0 === e[0] && !(128 & e[1])) throw new t("invalid signature integer: unnecessary leading zero");
                            return nK(e)
                        }
                    },
                    toSig(e) {
                        let {
                            Err: t,
                            _int: i,
