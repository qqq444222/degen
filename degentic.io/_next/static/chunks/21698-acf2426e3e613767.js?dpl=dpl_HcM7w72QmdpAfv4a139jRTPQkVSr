                er = en.reduce((e, t, i) => (e[i] = t, e), []),
                eo = en.reduce((e, t, i) => (e[t.codePointAt(0)] = i, e), []),
                es = S({
                    prefix: "\uD83D\uDE80",
                    name: "base256emoji",
                    encode: function(e) {
                        return e.reduce((e, t) => e += er[t], "")
                    },
                    decode: function(e) {
                        let t = [];
                        for (let i of e) {
                            let e = eo[i.codePointAt(0)];
                            if (void 0 === e) throw Error(`Non-base256emoji character: ${i}`);
                            t.push(e)
                        }
                        return new Uint8Array(t)
                    }
                }),
                ea = {
                    encode: function e(t, i, n) {
                        i = i || [];
                        for (var r = n = n || 0; t >= 0x80000000;) i[n++] = 255 & t | 128, t /= 128;
                        for (; - 128 & t;) i[n++] = 255 & t | 128, t >>>= 7;
                        return i[n] = 0 | t, e.bytes = n - r + 1, i
                    },
                    decode: function e(t, i) {
                        var n, r = 0,
                            i = i || 0,
                            o = 0,
                            s = i,
                            a = t.length;
                        do {
                            if (s >= a) throw e.bytes = 0, RangeError("Could not decode varint");
                            n = t[s++], r += o < 28 ? (127 & n) << o : (127 & n) * Math.pow(2, o), o += 7
                        } while (n >= 128);
                        return e.bytes = s - i, r
                    },
                    encodingLength: function(e) {
                        return e < 128 ? 1 : e < 16384 ? 2 : e < 2097152 ? 3 : e < 0x10000000 ? 4 : e < 0x800000000 ? 5 : e < 0x40000000000 ? 6 : e < 0x2000000000000 ? 7 : e < 0x100000000000000 ? 8 : e < 0x8000000000000000 ? 9 : 10
                    }
                },
                el = (e, t = 0) => [ea.decode(e, t), ea.decode.bytes],
                ec = (e, t, i = 0) => (ea.encode(e, t, i), t),
                ed = e => ea.encodingLength(e),
                eu = (e, t) => {
                    let i = t.byteLength,
                        n = ed(e),
                        r = n + ed(i),
                        o = new Uint8Array(r + i);
                    return ec(e, o, 0), ec(i, o, n), o.set(t, r), new ef(e, i, t, o)
                },
                eh = e => {
                    let t = v(e),
                        [i, n] = el(t),
                        [r, o] = el(t.subarray(n)),
                        s = t.subarray(n + o);
                    if (s.byteLength !== r) throw Error("Incorrect length");
                    return new ef(i, r, s, t)
                },
                ep = (e, t) => e === t || e.code === t.code && e.size === t.size && w(e.bytes, t.bytes);
            class ef {
                constructor(e, t, i, n) {
                    this.code = e, this.size = t, this.digest = i, this.bytes = n
                }
            }
            let eg = ({
                name: e,
                code: t,
                encode: i
            }) => new em(e, t, i);
            class em {
                constructor(e, t, i) {
                    this.name = e, this.code = t, this.encode = i
                }
                digest(e) {
                    if (e instanceof Uint8Array) {
                        let t = this.encode(e);
                        return t instanceof Uint8Array ? eu(this.code, t) : t.then(e => eu(this.code, e))
                    }
                    throw Error("Unknown type, must be binary type")
                }
            }
            let ey = e => async t => new Uint8Array(await crypto.subtle.digest(e, t)), eb = eg({
                name: "sha2-256",
                code: 18,
                encode: ey("SHA-256")
            }), ex = eg({
                name: "sha2-512",
                code: 19,
                encode: ey("SHA-512")
            }), ew = {
                code: 0,
                name: "identity",
                encode: v,
                digest: e => eu(0, v(e))
            }, ev = "raw", eC = 85, ej = e => v(e), eM = e => v(e), eI = new TextEncoder, eN = new TextDecoder, eD = "json", eE = 512, eS = e => eI.encode(JSON.stringify(e)), eA = e => JSON.parse(eN.decode(e));
            class eT {
                constructor(e, t, i, n) {
                    this.code = t, this.version = e, this.multihash = i, this.bytes = n, this.byteOffset = n.byteOffset, this.byteLength = n.byteLength, this.asCID = this, this._baseCache = new Map, Object.defineProperties(this, {
                        byteOffset: eU,
                        byteLength: eU,
                        code: eB,
                        version: eB,
                        multihash: eB,
                        bytes: eB,
                        _baseCache: eU,
                        asCID: eU
                    })
                }
                toV0() {
                    if (0 === this.version) return this;
                    {
                        let {
                            code: e,
                            multihash: t
                        } = this;
                        if (e !== eO) throw Error("Cannot convert a non dag-pb CID to CIDv0");
                        if (t.code !== eP) throw Error("Cannot convert non sha2-256 multihash CID to CIDv0");
                        return eT.createV0(t)
                    }
                }
                toV1() {
                    switch (this.version) {
                        case 0: {
                            let {
                                code: e,
                                digest: t
                            } = this.multihash, i = eu(e, t);
                            return eT.createV1(this.code, i)
                        }
                        case 1:
                            return this;
                        default:
                            throw Error(`Can not convert CID version ${this.version} to version 0. This is a bug please report`)
                    }
                }
                equals(e) {
                    return e && this.code === e.code && this.version === e.version && ep(this.multihash, e.multihash)
                }
                toString(e) {
                    let {
                        bytes: t,
                        version: i,
                        _baseCache: n
                    } = this;
                    return 0 === i ? eL(t, n, e || Z.encoder) : ez(t, n, e || U.encoder)
                }
                toJSON() {
                    return {
                        code: this.code,
                        version: this.version,
                        hash: this.multihash.bytes
                    }
                }
                get[Symbol.toStringTag]() {
                    return "CID"
                } [Symbol.for("nodejs.util.inspect.custom")]() {
                    return "CID(" + this.toString() + ")"
                }
                static isCID(e) {
                    return eW(/^0\.0/, eF), !!(e && (e[e_] || e.asCID === e))
                }
                get toBaseEncodedString() {
                    throw Error("Deprecated, use .toString()")
                }
                get codec() {
                    throw Error('"codec" property is deprecated, use integer "code" property instead')
                }
                get buffer() {
                    throw Error("Deprecated .buffer property, use .bytes to get Uint8Array instead")
                }
                get multibaseName() {
                    throw Error('"multibaseName" property is deprecated')
                }
                get prefix() {
                    throw Error('"prefix" property is deprecated')
                }
                static asCID(e) {
                    if (e instanceof eT) return e;
                    if (null != e && e.asCID === e) {
                        let {
                            version: t,
                            code: i,
                            multihash: n,
                            bytes: r
                        } = e;
                        return new eT(t, i, n, r || eR(t, i, n.bytes))
                    }
                    if (null == e || !0 !== e[e_]) return null;
                    {
                        let {
                            version: t,
                            multihash: i,
                            code: n
                        } = e, r = eh(i);
                        return eT.create(t, n, r)
                    }
                }
                static create(e, t, i) {
                    if ("number" != typeof t) throw Error("String codecs are no longer supported");
                    switch (e) {
                        case 0:
                            if (t === eO) return new eT(e, t, i, i.bytes);
                            throw Error(`Version 0 CID must use dag-pb (code: ${eO}) block encoding`);
                        case 1: {
                            let n = eR(e, t, i.bytes);
                            return new eT(e, t, i, n)
                        }
                        default:
                            throw Error("Invalid version")
                    }
                }
                static createV0(e) {
                    return eT.create(0, eO, e)
                }
                static createV1(e, t) {
                    return eT.create(1, e, t)
                }
                static decode(e) {
                    let [t, i] = eT.decodeFirst(e);
                    if (i.length) throw Error("Incorrect length");
                    return t
                }
                static decodeFirst(e) {
                    let t = eT.inspectBytes(e),
                        i = t.size - t.multihashSize,
                        n = v(e.subarray(i, i + t.multihashSize));
                    if (n.byteLength !== t.multihashSize) throw Error("Incorrect length");
                    let r = n.subarray(t.multihashSize - t.digestSize),
                        o = new ef(t.multihashCode, t.digestSize, r, n);
                    return [0 === t.version ? eT.createV0(o) : eT.createV1(t.codec, o), e.subarray(t.size)]
                }
                static inspectBytes(e) {
                    let t = 0,
                        i = () => {
                            let [i, n] = el(e.subarray(t));
                            return t += n, i
                        },
                        n = i(),
                        r = eO;
                    if (18 === n ? (n = 0, t = 0) : 1 === n && (r = i()), 0 !== n && 1 !== n) throw RangeError(`Invalid CID version ${n}`);
                    let o = t,
                        s = i(),
                        a = i(),
                        l = t + a;
                    return {
                        version: n,
                        codec: r,
                        multihashCode: s,
                        digestSize: a,
                        multihashSize: l - o,
                        size: l
                    }
                }
                static parse(e, t) {
                    let [i, n] = ek(e, t), r = eT.decode(n);
                    return r._baseCache.set(i, e), r
                }
            }
            let ek = (e, t) => {
                    switch (e[0]) {
                        case "Q": {
                            let i = t || Z;
                            return [Z.prefix, i.decode(`${Z.prefix}${e}`)]
                        }
                        case Z.prefix: {
                            let i = t || Z;
                            return [Z.prefix, i.decode(e)]
                        }
                        case U.prefix: {
                            let i = t || U;
                            return [U.prefix, i.decode(e)]
                        }
                        default:
                            if (null == t) throw Error("To parse non base32 or base58btc encoded CID multibase decoder must be provided");
                            return [e[0], t.decode(e)]
                    }
                },
                eL = (e, t, i) => {
                    let {
                        prefix: n
                    } = i;
                    if (n !== Z.prefix) throw Error(`Cannot string encode V0 in ${i.name} encoding`);
                    let r = t.get(n);
                    if (null != r) return r;
                    {
                        let r = i.encode(e).slice(1);
                        return t.set(n, r), r
                    }
                },
                ez = (e, t, i) => {
                    let {
                        prefix: n
                    } = i, r = t.get(n);
                    if (null != r) return r;
                    {
                        let r = i.encode(e);
                        return t.set(n, r), r
                    }
                },
                eO = 112,
                eP = 18,
                eR = (e, t, i) => {
                    let n = ed(e),
                        r = n + ed(t),
                        o = new Uint8Array(r + i.byteLength);
                    return ec(e, o, 0), ec(t, o, n), o.set(i, r), o
                },
                e_ = Symbol.for("@ipld/js-cid/CID"),
                eB = {
                    writable: !1,
                    configurable: !1,
                    enumerable: !0
                },
                eU = {
                    writable: !1,
                    enumerable: !1,
                    configurable: !1
                },
                eW = (e, t) => {
                    if (e.test("0.0.0-dev")) console.warn(t);
                    else throw Error(t)
                },
                eF = `CID.isCID(v) is deprecated and will be removed in the next major release.
Following code pattern:

if (CID.isCID(value)) {
  doSomethingWithCID(value)
}

Is replaced with:

const cid = CID.asCID(value)
if (cid) {
  // Make sure to use cid instead of value
  doSomethingWithCID(cid)
}
`,
                eY = {
                    ...n,
                    ...r,
                    ...o,
                    ...s,
                    ...a,
                    ...l,
                    ...c,
                    ...d,
                    ...u,
                    ...h
