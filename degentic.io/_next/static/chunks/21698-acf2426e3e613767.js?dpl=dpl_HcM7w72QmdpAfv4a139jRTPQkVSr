            async function tv({
                wallet: e,
                accountAddress: t,
                sourceSupportedTokens: i,
                toChain: n,
                toToken: r,
                tokenAmount: o,
                mode: s,
                client: c
            }) {
                let d = e.getAccount();
                if (!d) return [];
                let u = [],
                    h = Object.keys(i).map(e => (0, a.Q4)(Number(e))),
                    p = function(e) {
                        let t = [];
                        for (let i = 0; i < e.length; i += 5) t.push(e.slice(i, i + 5));
                        return t
                    }((await Promise.all(h.map(async e => ({
                        chain: e,
                        enabled: await (0, tx.isInsightEnabled)(e)
                    })))).filter(e => e.enabled).map(e => e.chain));
                await Promise.all(p.map(async e => {
                    for (let t of (await (0, tw.y)({
                            ownerAddress: d.address,
                            chains: e,
                            client: c
                        }))) {
                        let e = i[t.chainId]?.find(e => e.address.toLowerCase() === t.tokenAddress.toLowerCase());
                        e && u.push({
                            balance: t,
                            chain: (0, a.Q4)(t.chainId),
                            token: e
                        })
                    }
                }));
                let f = (0, V.rk)(r) ? {
                        address: l.DG,
                        name: n.nativeCurrency?.name || "",
                        symbol: n.nativeCurrency?.symbol || "",
                        icon: n.icon?.url
                    } : r,
                    g = {
                        ...i,
                        [n.id]: [f, ...i[n.id] || []]
                    },
                    m = [];
                for (let [e, i] of Object.entries(g)) {
                    let r = Number(e),
                        l = (0, a.Q4)(r);
                    for (let e of i) {
                        let i = (0, V.rk)(e),
                            a = u.some(t => t.chain.id === r && t.token.address.toLowerCase() === e.address.toLowerCase());
                        (i || a) && m.push((async () => {
                            try {
                                let r = await (0, ew.m)({
