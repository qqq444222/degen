                                s = w.fromHex(i, d), a = w.fromHex(e.slice(0, u), d), l = v.multiplyUnsafe(h)
                            } catch {
                                return !1
                            }
                            if (!d && s.isSmallOrder()) return !1;
                            let p = E(c, a.toRawBytes(), s.toRawBytes(), t);
                            return a.add(s.multiplyUnsafe(p)).subtract(l).clearCofactor().equals(w.ZERO)
                        },
                        ExtendedPoint: w,
                        utils: {
                            getExtendedPublicKey: N,
                            randomPrivateKey: () => a(n.BYTES),
                            precompute: (e = 8, t = w.BASE) => (t._setWindowSize(e), t.multiply(BigInt(3)), t)
                        }
                    }
                }({
                    a: BigInt(-1),
                    d: BigInt("37095705934669439343138083508754565189542113879843219016388785533085940283555"),
                    Fp: eM,
