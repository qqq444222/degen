            var eV = function(e, t) {
                if (e.length >= 255) throw TypeError("Alphabet too long");
                for (var i = new Uint8Array(256), n = 0; n < i.length; n++) i[n] = 255;
                for (var r = 0; r < e.length; r++) {
                    var o = e.charAt(r),
                        s = o.charCodeAt(0);
                    if (255 !== i[s]) throw TypeError(o + " is ambiguous");
                    i[s] = r
                }
                var a = e.length,
                    l = e.charAt(0),
                    c = Math.log(a) / Math.log(256),
                    d = Math.log(256) / Math.log(a);

                function u(e) {
                    if ("string" != typeof e) throw TypeError("Expected String");
                    if (0 === e.length) return new Uint8Array;
                    var t = 0;
                    if (" " !== e[0]) {
                        for (var n = 0, r = 0; e[t] === l;) n++, t++;
                        for (var o = (e.length - t) * c + 1 >>> 0, s = new Uint8Array(o); e[t];) {
                            var d = i[e.charCodeAt(t)];
                            if (255 === d) return;
                            for (var u = 0, h = o - 1;
                                (0 !== d || u < r) && -1 !== h; h--, u++) d += a * s[h] >>> 0, s[h] = d % 256 >>> 0, d = d / 256 >>> 0;
                            if (0 !== d) throw Error("Non-zero carry");
                            r = u, t++
                        }
                        if (" " !== e[t]) {
                            for (var p = o - r; p !== o && 0 === s[p];) p++;
                            for (var f = new Uint8Array(n + (o - p)), g = n; p !== o;) f[g++] = s[p++];
                            return f
                        }
                    }
                }
                return {
                    encode: function(t) {
                        if (t instanceof Uint8Array || (ArrayBuffer.isView(t) ? t = new Uint8Array(t.buffer, t.byteOffset, t.byteLength) : Array.isArray(t) && (t = Uint8Array.from(t))), !(t instanceof Uint8Array)) throw TypeError("Expected Uint8Array");
                        if (0 === t.length) return "";
                        for (var i = 0, n = 0, r = 0, o = t.length; r !== o && 0 === t[r];) r++, i++;
                        for (var s = (o - r) * d + 1 >>> 0, c = new Uint8Array(s); r !== o;) {
                            for (var u = t[r], h = 0, p = s - 1;
                                (0 !== u || h < n) && -1 !== p; p--, h++) u += 256 * c[p] >>> 0, c[p] = u % a >>> 0, u = u / a >>> 0;
                            if (0 !== u) throw Error("Non-zero carry");
                            n = h, r++
                        }
                        for (var f = s - n; f !== s && 0 === c[f];) f++;
