                    if (e.length !== t.length) return !1;
                    let i = 0;
                    for (let n = 0; n < e.length; n++) i |= e[n] ^ t[n];
                    return 0 === i
                },
                utf8ToBytes: function(e) {
                    if ("string" != typeof e) throw Error("string expected");
                    return new Uint8Array(new TextEncoder().encode(e))
                },
                inRange: i7,
                aInRange: ne,
                bitLen: nt,
                bitGet: function(e, t) {
                    return e >> BigInt(t) & iK
                },
                bitSet: function(e, t, i) {
                    return e | (i ? iK : iY) << BigInt(t)
                },
                bitMask: ni,
                createHmacDrbg: no,
                validateObject: na,
                notImplemented: () => {
                    throw Error("not implemented")
                },
                memoized: nl
            });
            let nd = BigInt(0),
                nu = BigInt(1),
                nh = BigInt(2),
                np = BigInt(3),
                nf = BigInt(4),
                ng = BigInt(5),
                nm = BigInt(8);

            function ny(e, t) {
                let i = e % t;
                return i >= nd ? i : t + i
            }

            function nb(e, t, i) {
                if (t < nd) throw Error("invalid exponent, negatives unsupported");
                if (i <= nd) throw Error("invalid modulus");
                if (i === nu) return nd;
                let n = nu;
                for (; t > nd;) t & nu && (n = n * e % i), e = e * e % i, t >>= nu;
                return n
            }

            function nx(e, t, i) {
                let n = e;
                for (; t-- > nd;) n *= n, n %= i;
                return n
            }

            function nw(e, t) {
                if (e === nd) throw Error("invert: expected non-zero number");
                if (t <= nd) throw Error("invert: expected positive modulus, got " + t);
                let i = ny(e, t),
                    n = t,
                    r = nd,
                    o = nu;
                for (; i !== nd;) {
                    let e = n / i,
                        t = n % i,
                        s = r - o * e;
                    n = i, i = t, r = o, o = s
                }
                if (n !== nu) throw Error("invert: does not exist");
                return ny(r, t)
            }
            let nv = ["create", "isValid", "is0", "neg", "inv", "sqrt", "sqr", "eql", "add", "sub", "mul", "pow", "div", "addN", "subN", "mulN", "sqrN"];

            function nC(e, t) {
                let i = void 0 !== t ? t : e.toString(2).length,
                    n = Math.ceil(i / 8);
                return {
                    nBitLength: i,
                    nByteLength: n
                }
            }

            function nj(e, t, i = !1, n = {}) {
                let r;
                if (e <= nd) throw Error("invalid field: expected ORDER > 0, got " + e);
                let {
                    nBitLength: o,
                    nByteLength: s
                } = nC(e, t);
                if (s > 2048) throw Error("invalid field: expected ORDER of <= 2048 bytes");
                let a = Object.freeze({
                    ORDER: e,
                    isLE: i,
                    BITS: o,
                    BYTES: s,
                    MASK: ni(o),
                    ZERO: nd,
                    ONE: nu,
                    create: t => ny(t, e),
                    isValid: t => {
                        if ("bigint" != typeof t) throw Error("invalid field element: expected bigint, got " + typeof t);
                        return nd <= t && t < e
                    },
                    is0: e => e === nd,
                    isOdd: e => (e & nu) === nu,
                    neg: t => ny(-t, e),
                    eql: (e, t) => e === t,
                    sqr: t => ny(t * t, e),
                    add: (t, i) => ny(t + i, e),
                    sub: (t, i) => ny(t - i, e),
                    mul: (t, i) => ny(t * i, e),
                    pow: (e, t) => (function(e, t, i) {
                        if (i < nd) throw Error("invalid exponent, negatives unsupported");
                        if (i === nd) return e.ONE;
                        if (i === nu) return t;
                        let n = e.ONE,
                            r = t;
                        for (; i > nd;) i & nu && (n = e.mul(n, r)), r = e.sqr(r), i >>= nu;
                        return n
                    })(a, e, t),
                    div: (t, i) => ny(t * nw(i, e), e),
                    sqrN: e => e * e,
                    addN: (e, t) => e + t,
                    subN: (e, t) => e - t,
                    mulN: (e, t) => e * t,
                    inv: t => nw(t, e),
                    sqrt: n.sqrt || (t => (r || (r = function(e) {
                        if (e % nf === np) {
                            let t = (e + nu) / nf;
                            return function(e, i) {
                                let n = e.pow(i, t);
                                if (!e.eql(e.sqr(n), i)) throw Error("Cannot find square root");
                                return n
                            }
                        }
                        if (e % nm === ng) {
                            let t = (e - ng) / nm;
                            return function(e, i) {
                                let n = e.mul(i, nh),
                                    r = e.pow(n, t),
                                    o = e.mul(i, r),
                                    s = e.mul(e.mul(o, nh), r),
                                    a = e.mul(o, e.sub(s, e.ONE));
                                if (!e.eql(e.sqr(a), i)) throw Error("Cannot find square root");
                                return a
                            }
                        }
                        return function(e) {
                            let t, i, n;
                            let r = (e - nu) / nh;
                            for (t = e - nu, i = 0; t % nh === nd; t /= nh, i++);
                            for (n = nh; n < e && nb(n, r, e) !== e - nu; n++)
                                if (n > 1e3) throw Error("Cannot find square root: likely non-prime P");
                            if (1 === i) {
                                let t = (e + nu) / nf;
                                return function(e, i) {
                                    let n = e.pow(i, t);
                                    if (!e.eql(e.sqr(n), i)) throw Error("Cannot find square root");
                                    return n
                                }
                            }
                            let o = (t + nu) / nh;
                            return function(e, s) {
                                if (e.pow(s, r) === e.neg(e.ONE)) throw Error("Cannot find square root");
                                let a = i,
                                    l = e.pow(e.mul(e.ONE, n), t),
                                    c = e.pow(s, o),
                                    d = e.pow(s, t);
                                for (; !e.eql(d, e.ONE);) {
                                    if (e.eql(d, e.ZERO)) return e.ZERO;
                                    let t = 1;
                                    for (let i = e.sqr(d); t < a && !e.eql(i, e.ONE); t++) i = e.sqr(i);
                                    let i = e.pow(l, nu << BigInt(a - t - 1));
                                    l = e.sqr(i), c = e.mul(c, i), d = e.mul(d, l), a = t
                                }
                                return c
                            }
                        }(e)
                    }(e)), r(a, t))),
                    invertBatch: e => (function(e, t) {
                        let i = Array(t.length),
                            n = t.reduce((t, n, r) => e.is0(n) ? t : (i[r] = t, e.mul(t, n)), e.ONE),
                            r = e.inv(n);
                        return t.reduceRight((t, n, r) => e.is0(n) ? t : (i[r] = e.mul(t, i[r]), e.mul(t, n)), r), i
                    })(a, e),
                    cmov: (e, t, i) => i ? t : e,
                    toBytes: e => i ? i3(e, s) : i5(e, s),
                    fromBytes: e => {
                        if (e.length !== s) throw Error("Field.fromBytes: expected " + s + " bytes, got " + e.length);
                        return i ? i4(e) : i2(e)
                    }
                });
                return Object.freeze(a)
            }

            function nM(e) {
                if ("bigint" != typeof e) throw Error("field order must be bigint");
                return Math.ceil(e.toString(2).length / 8)
            }

            function nI(e) {
                let t = nM(e);
                return t + Math.ceil(t / 2)
            }
            let nN = BigInt(0),
                nD = BigInt(1);

            function nE(e, t) {
                let i = t.negate();
                return e ? i : t
            }

            function nS(e, t) {
                if (!Number.isSafeInteger(e) || e <= 0 || e > t) throw Error("invalid window size, expected [1.." + t + "], got W=" + e)
            }

            function nA(e, t) {
                return nS(e, t), {
                    windows: Math.ceil(t / e) + 1,
                    windowSize: 2 ** (e - 1)
                }
            }
            let nT = new WeakMap,
                nk = new WeakMap;

            function nL(e) {
                return nk.get(e) || 1
            }

            function nz(e) {
                return na(e.Fp, nv.reduce((e, t) => (e[t] = "function", e), {
                    ORDER: "bigint",
