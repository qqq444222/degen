            function l(e) {
                var t = e.length;
                if (t % 4 > 0) throw Error("Invalid string. Length must be a multiple of 4");
                var i = e.indexOf("="); - 1 === i && (i = t);
                var n = i === t ? 0 : 4 - i % 4;
