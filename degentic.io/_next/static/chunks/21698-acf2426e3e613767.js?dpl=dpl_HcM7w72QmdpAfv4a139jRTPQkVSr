                })
            }
            BigInt(0), BigInt(1), BigInt(2), BigInt(8);
            let nO = BigInt(0),
                nP = BigInt(1),
                nR = BigInt("57896044618658097711785492504343953926634992332820282019728792003956564819949");
            BigInt(0);
            let n_ = BigInt(1),
                nB = BigInt(2),
                nU = BigInt(3),
                nW = BigInt(5);
            BigInt(8);
            let nF = function(e) {
                let t = (na(e, {
                        a: "bigint"
