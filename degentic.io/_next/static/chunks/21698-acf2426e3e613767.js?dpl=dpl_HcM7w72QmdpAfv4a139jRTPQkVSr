                    e[1] &= 8191;
                    for (let t = 2; t < 10; t++) e[t] += n, n = e[t] >>> 13, e[t] &= 8191;
                    e[0] += 5 * n, n = e[0] >>> 13, e[0] &= 8191, e[1] += n, n = e[1] >>> 13, e[1] &= 8191, e[2] += n, i[0] = e[0] + 5, n = i[0] >>> 13, i[0] &= 8191;
