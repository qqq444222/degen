                    hash: v,
                    randomBytes: u,
                    adjustScalarBytes: function(e) {
                        return e[0] &= 248, e[31] &= 127, e[31] |= 64, e
                    },
                    uvRatio: function(e, t) {
                        let i = J(t * t * t, eb),
                            n = function(e) {
                                let t = BigInt(10),
                                    i = BigInt(20),
                                    n = BigInt(40),
                                    r = BigInt(80),
                                    o = e * e % eb * e % eb,
                                    s = X(o, ev, eb) * o % eb,
                                    a = X(s, ew, eb) * e % eb,
                                    l = X(a, eC, eb) * a % eb,
                                    c = X(l, t, eb) * l % eb,
