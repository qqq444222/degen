                                })
                            } catch (t) {
                                console.warn(`Failed to fetch balance for ${e.symbol} on chain ${r}`, t)
                            }
                        })())
                    }
                }
                await Promise.all(m);
                {
                    let e = {};
                    for (let t of u) {
                        let i = `${t.chain.id}-${t.token.address.toLowerCase()}`;
                        e[i] || (e[i] = t)
                    }
                    u.splice(0, u.length, ...Object.values(e))
                }
                return u.sort((e, t) => {
                    let i = f.address;
                    return e.chain.id === n.id && e.token.address === i ? -1 : t.chain.id === n.id && t.token.address === i ? 1 : e.chain.id === n.id ? -1 : t.chain.id === n.id ? 1 : e.chain.id - t.chain.id
                }), u
            }

