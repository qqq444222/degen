                                    h = e % 2 != 0,
                                    p = r < 0;
                                0 === r ? a = a.add(ea(h, i[t])) : s = s.add(ea(p, i[u]))
                            }
                            return {
                                p: s,
                                f: a
                            }
                        },
                        wNAFUnsafe(e, i, n, r = w.ZERO) {
                            let {
                                windows: o,
                                windowSize: s
                            } = ec(e, t), a = BigInt(2 ** e - 1), l = 2 ** e, c = BigInt(e);
                            for (let e = 0; e < o; e++) {
                                let t = e * s;
                                if (n === eo) break;
                                let o = Number(n & a);
                                if (n >>= c, o > s && (o -= l, n += es), 0 === o) continue;
                                let d = i[t + Math.abs(o) - 1];
                                o < 0 && (d = d.negate()), r = r.add(d)
                            }
                            return r
                        },
                        getPrecomputes(e, t, i) {
                            let n = ed.get(t);
                            return n || (n = this.precomputeWindow(t, e), 1 !== e && ed.set(t, i(n))), n
                        },
                        wNAFCached(e, t, i) {
                            let n = eh(e);
                            return this.wNAF(n, this.getPrecomputes(n, e, i), t)
                        },
                        wNAFCachedUnsafe(e, t, i, n) {
                            let r = eh(e);
                            return 1 === r ? this.unsafeLadder(e, t, n) : this.wNAFUnsafe(r, this.getPrecomputes(r, e, i), t, n)
                        },
                        setWindowSize(e, i) {
                            el(i, t), eu.set(e, i), ed.delete(e)
                        }
                    });

                    function N(e) {
                        let t = n.BYTES;
                        e = R("private key", e, t);
                        let i = R("hashed private key", s(e), 2 * t),
                            o = f(i.slice(0, t)),
                            a = i.slice(t, 2 * t),
                            l = J(z(o), r),
                            c = v.multiply(l),
                            d = c.toRawBytes();
                        return {
                            head: o,
                            prefix: a,
                            scalar: l,
                            point: c,
                            pointBytes: d
                        }
                    }

                    function E(e = new Uint8Array, ...t) {
                        return J(z(s(g(_(...t), R("context", e), !!o))), r)
                    }
                    return v._setWindowSize(8), {
                        CURVE: i,
                        getPublicKey: function(e) {
                            return N(e).pointBytes
                        },
                        sign: function(e, t, i = {}) {
                            e = R("message", e), o && (e = o(e));
                            let {
