                            return U("signature.s", u, ep, r), R("result", _(d, P(u, n.BYTES)), 2 * n.BYTES)
                        },
                        verify: function(e, t, i, r = ey) {
                            let s, a, l;
                            let {
                                context: c,
                                zip215: d
                            } = r, u = n.BYTES;
